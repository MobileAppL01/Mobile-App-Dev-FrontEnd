{
  "expo": {
    "name": "template-onboarding",
    "slug": "bookingtonmobiledev",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/icons/app_icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./src/assets/icons/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.khoale2k4.templateonboarding",
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/icons/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "favicon": "./src/assets/icons/favicon.png"
    },
    "extra": {
      "eas": {},
      "router": {}
    },
    "plugins": [
      "expo-font",
      "expo-router",
      "expo-web-browser",
      [
        "@sentry/react-native/expo",
        {
          "url": "https://sentry.io/",
          "project": "react-native-mobile-app",
          "organization": "hcmut-nu"
        }
      ]
    ],
    "runtimeVersion": {
      "policy": "appVersion"
    }
  }
}