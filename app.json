{
  "expo": {
    "name": "Bookinton",
    "slug": "bookingtonmobiledev",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/icons/app_icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/icons/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.bookinton.mobile",
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/icons/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "minSdkVersion": 24
    },
    "newArchEnabled": true,
    "web": {
      "favicon": "./src/assets/icons/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "a703c044-9af5-46ed-bf0a-271050cfda85"
      },
      "router": {}
    },
    "plugins": [
      "expo-font",
      "expo-router",
      [
        "expo-build-properties",
        {
          "android": {
            "useLegacyPackaging": true,
            "ndkVersion": "27.0.12077973"
          }
        }
      ],
      "@react-native-community/datetimepicker",
      [
        "@sentry/react-native/expo",
        {
          "url": "https://sentry.io/",
          "project": "react-native-mobile-app",
          "organization": "hcmut-nu"
        }
      ]
    ],
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "owner": "clearwind"
  }
}
